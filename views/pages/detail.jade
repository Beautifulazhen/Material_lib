extends ../layout

block content
	.container
		.row
			.col-md-7
				img(src='#{material.img}' widith="800" height="400")
			.col-md-5
				dl.dl-horizontal
					dt 名字
					dd #{material.title}
					dt 描述
					dd #{material.description}
					dt 来源
					dd #{material.src}
					dt 本地路径
					dd #{material.local}
				